{
  "meta": {
    "module": "openc2",
    "title": "OpenC2 Command Definitions",
    "version": "wd05",
    "description": "Datatypes that define the content of OpenC2 commands and responses."
  },
  "types": {
    "OpenC2_Command": {
      "id": {
        "type": "string"
      },
      "action": {
        "$ref": "#/types/Action"
      },
      "target": {
        "$ref": "#/types/Target"
      },
      "actuator": {
        "$ref": "#/types/Actuator"
      },
      "options": {
        "$ref": "#/types/Command_Options"
      }
    },
    "OpenC2Response": {
      "id": {
        "type": "string"
      },
      "status": {
        "$ref": "#/types/Status_Code"
      },
      "status_text": {
        "type": "string"
      },
      "result": {
        "$ref": "#/types/Results"
      }
    },
    "Results": {
      "oneOf": [
        {
          "string": {
            "type": "string"
          }
        },
        {
          "media": {
            "$ref": "#/types/Media"
          }
        }
      ]
    },
    "Media": {
      "media_type": {
        "type": "string"
      },
      "data": {
        "$ref": "#/types/Payload"
      }
    },
    "Action": {
      "enum": [
        "scan",
        "locate",
        "query",
        "report",
        "notify",
        "deny",
        "contain",
        "allow",
        "start",
        "stop",
        "restart",
        "pause",
        "resume",
        "cancel",
        "set",
        "update",
        "move",
        "redirect",
        "create",
        "delete",
        "snapshot",
        "detonate",
        "restore",
        "save",
        "throttle",
        "delay",
        "substitute",
        "copy",
        "sync",
        "investigate",
        "mitigate",
        "remediate"
      ]
    },
    "Target": {
      "oneOf": [
        {
          "artifact": {
            "$ref": "#/types/Artifact"
          }
        },
        {
          "command": {
            "type": "string"
          }
        },
        {
          "device": {
            "$ref": "#/types/Device"
          }
        },
        {
          "directory": {
            "type": "string"
          }
        },
        {
          "disk": {
            "type": "string"
          }
        },
        {
          "disk_partition": {
            "type": "string"
          }
        },
        {
          "domain_name": {
            "type": "string"
          }
        },
        {
          "email_addr": {
            "type": "string"
          }
        },
        {
          "email_message": {
            "type": "string"
          }
        },
        {
          "file": {
            "$ref": "#/types/File"
          }
        },
        {
          "ipv4_addr": {
            "type": "string"
          }
        },
        {
          "ipv6_addr": {
            "type": "string"
          }
        },
        {
          "mac_addr": {
            "type": "string"
          }
        },
        {
          "memory": {
            "type": "string"
          }
        },
        {
          "ip_connection": {
            "$ref": "#/types/Ip_Connection"
          }
        },
        {
          "openc2": {
            "$ref": "#/types/Openc2"
          }
        },
        {
          "process": {
            "type": "string"
          }
        },
        {
          "software": {
            "type": "string"
          }
        },
        {
          "uri": {
            "type": "string"
          }
        },
        {
          "user_account": {
            "type": "string"
          }
        },
        {
          "user_session": {
            "type": "string"
          }
        },
        {
          "volume": {
            "type": "string"
          }
        },
        {
          "windows_registry_key": {
            "type": "string"
          }
        },
        {
          "x509_certificate": {
            "type": "string"
          }
        }
      ]
    },
    "Actuator": {
      "oneOf": [
        {
          "any": "#/types/Actuator_Specifiers"
        },
        {
          "firewall": "#/types/Actuator_Specifiers"
        }
      ]
    },
    "Actuator_Specifiers": {
      "actuator_id": {
        "type": "string"
      },
      "asset_id": {
        "type": "string"
      }
    },
    "Command_Options": {
      "type": "object",
      "properties": {
        "start_time": {
          "type": "date_time"
        },
        "end_time": {
          "type": "date_time"
        },
        "duration": {
          "type": "string"
        },
        "response_requested": {
          "$ref": "#/types/Response_type"
        },
        "command_id": {
          "type": "string"
        }
      }
    },
    "Response_Type": {
      "enum": [
        "ack",
        "status"
      ]
    },
    "Status_Code": {
      "enum": [
        "102",
        "200",
        "301",
        "400",
        "401",
        "403",
        "500",
        "501"
      ]
    },
    "Hashes": {
      "type": "object",
      "properties": {
        "MD5": {
          "type": "binary"
        },
        "MD6": {
          "type": "binary"
        },
        "RIPEMD_160": {
          "type": "binary"
        },
        "SHA_1": {
          "type": "binary"
        },
        "SHA_224": {
          "type": "binary"
        },
        "SHA_256": {
          "type": "binary"
        },
        "SHA_384": {
          "type": "binary"
        },
        "SHA_512": {
          "type": "binary"
        },
        "SHA3_224": {
          "type": "binary"
        },
        "SHA3_256": {
          "type": "binary"
        },
        "SHA3_384": {
          "type": "binary"
        },
        "SHA3_512": {
          "type": "binary"
        },
        "ssdeep": {
          "type": "binary"
        },
        "WHIRLPOOL": {
          "type": "binary"
        }
      }
    },
    "Artifact": {
      "mime_type": {
        "type": "string"
      },
      "payload": {
        "$ref": "#/types/Payload"
      },
      "hashes": {
        "$ref": "#/types/Hashes"
      }
    },
    "Payload": {
      "oneOf": [
        {
          "payload_bin": "binary"
        },
        {
          "url": "uri"
        }
      ]
    },
    "Device": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "device_type": {
          "type": "string"
        },
        "manufacturer": {
          "type": "string"
        },
        "model": {
          "type": "string"
        },
        "serial_number": {
          "type": "string"
        },
        "firmware_version": {
          "type": "string"
        },
        "system_details": {
          "type": "string"
        }
      }
    },
    "File": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "path": {
          "type": "string"
        },
        "hash": {
          "$ref": "#/types/Hashes"
        },
        "reputation": {
          "$ref": "#/types/Reputation"
        }
      }
    },
    "Reputation": {
      "enum": [
        "0",
        "1",
        "2",
        "3",
        "4",
        "5",
        "6",
        "7"
      ]
    },
    "Ip_Connection": {
      "src_addr": {
        "type": "string"
      },
      "src_port": {
        "type": "string"
      },
      "dst_addr": {
        "type": "string"
      },
      "dst_port": {
        "type": "string"
      },
      "layer4_protocol": {
        "$ref": "#/types/Layer4_Protocol"
      }
    },
    "Layer4_Protocol": {
      "enum": [
        "icmp",
        "tcp",
        "udp",
        "sctp"
      ]
    },
    "Layer7_Protocol": {
      "enum": [
        "ftp_data",
        "ftp",
        "ssh",
        "telnet",
        "smtp",
        "http",
        "https"
      ]
    },
    "Openc2": {
      "oneOf": [
        {
          "actions": "string"
        },
        {
          "schema": "string"
        },
        {
          "actuator_groups": "string"
        }
      ]
    }
  }
}